{
  "name": "fastify-api",
  "version": "1.0.0",
  "description": "API for create, read, update and delete posts",
  "scripts": {
    "postinstall": "prisma generate",
    "postbuild": "prisma migrate deploy",
    "prepare": "husky",
    "lint": "eslint src",
    "format": "prettier --write \"./src/**/*.(js|jsx|ts|tsx|md|json)\"",
    "commitlint": "commitlint --edit",
    "precommit": "lint-staged",
    "prepush": "yarn test",
    "test": "jest"
  },
  "main": "src/server.js",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@eslint/js": "9.18.0",
    "@swc/core": "1.10.7",
    "@swc/jest": "0.2.37",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.6",
    "@typescript-eslint/eslint-plugin": "8.20.0",
    "@typescript-eslint/parser": "8.20.0",
    "eslint": "9.18.0",
    "eslint-config-prettier": "10.0.1",
    "globals": "15.14.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "lint-staged": "15.3.0",
    "prettier": "3.4.2",
    "prisma": "6.2.1",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "tsup": "8.3.5",
    "typescript": "5.7.3",
    "typescript-eslint": "8.20.0"
  },
  "dependencies": {
    "@prisma/client": "6.2.1",
    "dotenv": "16.4.7",
    "fastify": "5.2.1",
    "zod": "3.24.1"
  }
}
